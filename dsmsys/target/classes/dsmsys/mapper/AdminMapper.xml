<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dsmsys.dao.AdminDao">
	<select id="getAdminByMobileAndPwd"  parameterType="String" resultType="Admin">
		select * from admin where adm_mobile=#{admMobile} and adm_password=#{admPassword}
	</select>
	
</mapper>